{
  "extends": "@prompt-dev/typescript-config/node.json",
  "compilerOptions": {
    "outDir": "./dist",
    "moduleResolution": "nodenext",
    "noEmit": false,
    "paths": {
      "@prompt-dev/api": ["./src/api"],
      "@prompt-dev/api/*": ["./src/api/*"]
    }
  },
  "include": ["src"],
  "exclude": ["dist", "node_modules"]
}
